RULE_LIST	= RULE+ 
RULE		= RULE_NAME '=' EXPRESSION+ EOL
EXPRESSION	= TERM+ | '|' TERM+
TERM		= VALUE
ALTERNATION = '|'
ASSIGNMENT  = '='
NEGATION	= '-'
EOL			= '\n'
DELIMITER   = "' '" | '\t' | 'EOL'
REGEX		= '~'
LITERAL     = "~'(.+)'"
QUOTECHAR	= '\"'
EMPTY		= ''
ESCAPE		= '\\'
ZERO_MORE	= '*'
ONE_MORE	= '+'
ZERO_ONE	= '?'