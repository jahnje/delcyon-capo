RULE_LIST	= RULE | RULE RULE_LIST
RULE		= RULE_NAME ASSIGNMENT EXPRESSION EOL
EXPRESSION	= LIST | LIST ALTERNATION EXPRESSION
LIST		= TERM | TERM LIST
ALTERNATION = '|'
ASSIGNMENT  = '='
NEGATION	= '-'
EOL			= '\n'
DELIMITER   = "' '" | '\t' | 'EOL'
REGEX		= '~'
LITERAL     = "~'(.+)'"
QUOTECHAR	= "\"
EMPTY		= ''
ESCAPE		= '\'
ZERO_MORE	= '*'
ONE_MORE	= '+'
ZERO_ONE	= '?'
TEST		= "~This is some crazy test"